{
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/client/dist/assets/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/dist/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/client/dist/index.html"
      }
    ]
  }